export PATH=$HOME/bin:/usr/local/bin:$HOME/opt:$PATH
#export PATH=/usr/local/mysql-8.0.19-macos10.15-x86_64/bin:$PATH

export PATH="$HOME/.homebrew/bin:$HOME/.homebrew/sbin:$PATH"
#export PATH="$HOME/.homebrew/opt/qt/bin:$PATH"

export PATH="$PATH:/node_modules/.bin"
export PATH="$HOME/.nvm/versions/node/$(cat $HOME/.nvm/alias/default)/bin:$PATH"
#export PATH="$PATH:$HOME/opt/protoc-3.5.1-osx-x86_64/bin"
#export PATH="$PATH:$HOME/opt/nvim-osx64/bin"
export PATH="$HOME/.pyenv/versions/3.10.3/bin:$PATH"

export EDITOR='vim'

#export JAVA_HOME=$(/usr/libexec/java_home)
export PATH="$HOME/.homebrew/opt/openjdk/bin:$PATH"
#export CPPFLAGS="-I $HOME/.homebrew/opt/openjdk/include"

export GOROOT="$(brew --prefix golang)/libexec"
export PATH=$PATH:$GOROOT/bin
export GOPATH=$HOME/go
export PATH=$PATH:$GOPATH/bin

export PATH="$PATH:$HOME/.rvm/bin"
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

export NVM_DIR=$HOME/.nvm
source $(brew --prefix nvm)/nvm.sh

export CLOUDSDK_PYTHON="$HOME/.pyenv/versions/3.10.3/bin/python"
if [ -f "$HOME/opt/google-cloud-sdk/path.zsh.inc" ]; then source "$HOME/opt/google-cloud-sdk/path.zsh.inc"; fi
if [ -f "$HOME/opt/google-cloud-sdk/completion.zsh.inc" ]; then source "$HOME/opt/google-cloud-sdk/completion.zsh.inc"; fi
